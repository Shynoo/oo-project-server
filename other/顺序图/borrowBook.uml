@startuml

skinparam sequenceParticipant underline
!include style.ptmono.iuml

actor User as u
participant Controller as a
participant "UserService" as b
database BorrowingDao as d

u -> a : borrow(userId, bookId)
activate a
a->b:borrowBook(userId, bookId)

activate b

b->d:borrowOut(userId, bookId)

b-->a:result
deactivate b

a-->u:result
deactivate a

@enduml
